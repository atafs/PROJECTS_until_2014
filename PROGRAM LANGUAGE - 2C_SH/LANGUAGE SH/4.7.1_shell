#!/bin/bash

#4.7 ‐ Separação de palavras	
  
#Evidentemente que exercícios como o anterior são mais fáceis de realizar usando os
#mecanismos da shell que naturalmente separam palavras; ex:






echo -n "String: "
read s
p=""
for i in $s ; do
	if [ -z $p ] ; then
		p=$i; echo "Primeiro: $p"
	fi
done
echo "Ultimo: $i"

