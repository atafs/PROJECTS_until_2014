#!/bin/bash



if test $# -ge 1			#test é um comando... $1 é o primeiro argumento de um script
then
	echo "Posso continuar"
else
	echo "Faltam argumentos"
	exit 1
fi


echo "Arg1: $0"
echo "Arg1: $1"
echo "Cardinal: $#"
echo "asterisco: $*"



#vai mandar para o bash interpretar... podia interpretar com piten, etc...
