#!/bin/bash



if [ $# -z 1 ];then			#test é um comando... $1 é o primeiro argumento de um script

	echo "Posso continuar"
else
	echo "Faltam argumentos"
	exit 1
fi


echo "Arg1: $0"
echo "Arg1: $1"
echo "Cardinal: $#"
echo "asterisco: $*"


